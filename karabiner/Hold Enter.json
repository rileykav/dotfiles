{ "title": "Rules for Karabiner-Elements | Tested Version: 11.6.0",
  "rules": [
     { "description": "Hold a Key - Rule - Karabiner-Elements",
       "manipulators": [
        {"from": 
            { "key_code": "non_us_backslash" },
        "parameters": {"basic.to_if_alone_timeout_milliseconds": 300,
        "basic.to_if_held_down_threshold_milliseconds": 500},
        "to_if_alone": [
        {"key_code": "non_us_backslash"}],
          "to_if_held_down":[
                 {
"shell_command": "zsh $HOME/.dotfiles/karabiner/scripts/test.sh"
}],
          "type": "basic"
                }
            ]
        }
    ]
}
